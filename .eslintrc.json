{
	"parser": "babel-eslint",
	"extends": [
		"eslint:recommended",
		"plugin:react/recommended"
	],
	"env": {
		"browser": true,
		"jest": true,
		"es6": true,
		"node": true
	},
	"rules": {
		"react/prop-types": 0,
		"no-unused-vars": "warn"
	},
	"settings": {
		"react": {
			"version": "detect"
		}
	}
}